# AWS RDS PostgreSQL
# Get these from RDS Console after creating the database
DATABASE_URL=postgresql://username:password@10kay-db.xxxxxx.us-east-1.rds.amazonaws.com:5432/10kay

# Or split into components (alternative to DATABASE_URL):
# DB_HOST=10kay-db.xxxxxx.us-east-1.rds.amazonaws.com
# DB_PORT=5432
# DB_NAME=10kay
# DB_USER=postgres
# DB_PASSWORD=your-secure-password

# AWS Credentials
# Get these when you create the IAM user (you only see Secret Key once!)
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1

# S3 Buckets
# Use the bucket names you create (must be globally unique)
AWS_S3_FILINGS_BUCKET=10kay-filings
AWS_S3_AUDIO_BUCKET=10kay-audio

# AWS Bedrock
# Model ID for Claude Sonnet 4.5 (uses cross-region inference profile)
AWS_BEDROCK_MODEL_ID=us.anthropic.claude-sonnet-4-5-20250929-v1:0

# Finnhub (get free API key from https://finnhub.io/register)
# Used to fetch actual company-announced earnings dates
FINHUB_API_KEY=your_finnhub_api_key_here

# Clerk (get from https://dashboard.clerk.com after signup)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Stripe (get from https://dashboard.stripe.com after signup)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Resend (get from https://resend.com/api-keys after signup)
RESEND_API_KEY=re_...

# App Config
NEXT_PUBLIC_APP_URL=http://localhost:3000

# ElevenLabs (Phase 5 - get from https://elevenlabs.io)
# ELEVENLABS_API_KEY=...
# ELEVENLABS_VOICE_ID=...
